    .heading-about
      .container
        .row
          .col-lg-8.col-lg-offset-2
            .wow.fadeInDown(data-wow-delay='0.2s')
              .section-heading
                h1
    br
    .contianer
      .row
        .col-xs-8.col-xs-offset-2(style='text-align: center;')
          h3
          h2 Enter your current password and the password you want to change it to!
      br
      .row
        .col-xs-4.col-xs-offset-4
          form#resetOldPassword-form.resetOldPasswordFrom(action='', method='post', role='form')
            br
            .row
              .col-xs-6.col-xs-offset-3
                | Current Password
                input#oldPassword.form-control(type='password', name='currentPW' placeholder='Enter Password')
            br
            .row
              .col-xs-6.col-xs-offset-3
                | New Password
                input#newPassword.form-control(type='password', name='newPW' placeholder='Must at least 6 characters')
            br
            .row
              .col-xs-6.col-xs-offset-3
                | Retype New Password
                input#retype.form-control(type='password', name='newPWConfirm' placeholder='Must at least 6 characters')
            br
            br
            .row
              .col-md-12
                a(href='index.html')
                  button#referralSubmit.btn.btn-skin(type='submit') Submit!
