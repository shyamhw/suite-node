    .col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2.col-lg-4.col-lg-offset-4.form-box
      - var m = message
      if m != ""
        .alert.alert-danger #{message}
      form.f1(role='form', action='/registration', method='post', id='reg-form')
        h3 Create Account
        .f1-steps
          .f1-progress
            .f1-progress-line(data-now-value='16.66', data-number-of-steps='3', style='width: 16.66%;')
          .f1-step.active
            .f1-step-icon
              i.fa.fa-user
            p About
          .f1-step
            .f1-step-icon
              i.fa.fa-home
            p Account
          .f1-step
            .f1-step-icon
              i.fa.fa-money
            p Payment
        fieldset
          //- - var m = message
          //- if m != ""
          //-   .alert.alert-danger #{message}
          #alert_placeholder1
          .form-group
            | Email Address (username)
            input.f1-email.form-control(type='text', name='email', id ='email', placeholder='example@email.com')
          .form-group
            | First Name
            input.f1-first-name.form-control(type='text', name='f1firstname', id='fname', placeholder='John', maxlength='15')
          .form-group
            | Last Name
            input.f1-last-name.form-control(type='text', name='f1lastname', id='lname', placeholder='Smith', maxlength='15')
          .form-group
            | Password
            input.f1-password.form-control(type='password', name='password', placeholder='At least 7 characters', id="txtNewPassword")
          .form-group
            | Retype Password
            input.f1-retype.form-control(type='password', name='f1retype', id="txtConfirmPassword", onChange="checkPasswordMatch()")
